      </creator>
      <publisher>
        <name vocabulary="ADN">University Corporation for Atmospheric Research (UCAR)/Unidata</name>
        <contact url="http://www.unidata.ucar.edu/" email="support@unidata.ucar.edu"/>
      </publisher>
      <timeCoverage>
        <end>present</end>
        <duration>1 day</duration>
      </timeCoverage>
    </metadata>
