<?xml version="1.0" encoding="UTF-8"?>
<catalog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://www.unidata.ucar.edu/namespaces/thredds/InvCatalog/v1.0"
         xmlns:xlink="http://www.w3.org/1999/xlink"
         name="NCAR DSS THREDDS Data Server - Reanalysis data"
         version="1.0.6">

  <service name="latest" serviceType="Resolver" base=""/>

  <service name="fullServices" serviceType="Compound" base="">
    <service name="ncdods" serviceType="OPENDAP" base="/thredds/dodsC/"/>
    <service name="HTTPServer" serviceType="HTTPServer" base="/thredds/fileServer/"/>
    <service name="wcs" serviceType="WCS" base="/thredds/wcs/"/>
    <service name="wms" serviceType="WMS" base="/thredds/wms/"/>
    <service name="ncss" serviceType="NetcdfSubset" base="/thredds/ncss/"/>
    <service name="cdmremote" serviceType="CdmRemote" base="/thredds/cdmremote/"/>
    <service name="ncml" serviceType="NCML" base="/thredds/ncml/"/>
    <service name="uddc" serviceType="UDDC" base="/thredds/uddc/"/>
    <service name="iso" serviceType="ISO" base="/thredds/iso/"/>
  </service>

  <dataset name="Reanalysis Datasets">
    <metadata inherited="true">
      <serviceName>fullServices</serviceName>
      <authority>edu.ucar.ncar</authority>
      <dataType>GRID</dataType>
      <dataFormat>GRIB-1</dataFormat>
      <documentation xlink:href="http://rda.ucar.edu/" xlink:title="NCAR Research Data Archive"/>
      <documentation type="rights">Freely available</documentation>
      <documentation type="processing_level">Read by CDM Grib Collection, version 2.</documentation>
      <documentation xlink:href="http://rda.ucar.edu/datasets/ds335.0/" xlink:title="CISL Research Data Archive documentation"/>
      <documentation type="summary">Near Real-time Online THORPEX Interactive Grand Global Ensemble (TIGGE) Data
        This dataset holds the most recent 2-3 months of THORPEX Interactive Grand Global Ensemble (TIGGE) forecast data online for internet download.
      </documentation>
    </metadata>

    <featureCollection name="NCEP/NCAR Reanalysis Monthly Mean Subsets (ds090.2)" featureType="GRIB1" harvest="true" path="grib/ds090.2">
      <metadata inherited="true">
        <documentation type="summary">NCEP/NCAR Reanalysis Monthly Mean Subsets (from DS090.0), 1948-continuing (ds090.2)</documentation>
       </metadata>

      <collection spec="/glade/p/rda/data/ds090.2/**/.*"
                  name="ds090.2"
                  timePartition="directory"
                  olderThan="5 min"/>
      <update startup="never" trigger="allow"/>
      <tdm rewrite="test" rescan="0 1 3 * * ? *" trigger="allow"/>
      <gribConfig datasetTypes="TwoD Best"/>
    </featureCollection>

    <featureCollection name="JRA-55: Japanese 55-year Reanalysis, Monthly Means and Variances (ds628.1)" featureType="GRIB1" harvest="true" path="grib/ds628.1">
      <metadata inherited="true">
        <documentation type="summary">JRA-55: Japanese 55-year Reanalysis, Monthly Means and Variances (ds628.1)</documentation>
       </metadata>

      <collection spec="/glade/p/rda/data/ds628.1/**/.*"
                  name="ds628.1"
                  timePartition="directory"
                  olderThan="5 min"/>
      <update startup="never" trigger="allow"/>
      <tdm rewrite="test" rescan="0 1 3 * * ? *" trigger="allow"/>
      <gribConfig datasetTypes="TwoD Best"/>
    </featureCollection>

    <featureCollection name="NCEP Climate Forecast System Version 2 (CFSv2) Selected Hourly Time-Series Products (ds094.1)" featureType="GRIB2" harvest="true" path="grib/ds094.1">
      <metadata inherited="true">
        <documentation type="summary">NCEP Climate Forecast System Version 2 (CFSv2) Selected Hourly Time-Series Products (ds094.1)</documentation>
        <dataFormat>GRIB-2</dataFormat>
      </metadata>

      <collection spec="/glade/p/rda/data/ds094.1/**/.*grb2$"
                  name="ds094.1"
                  timePartition="directory"
                  olderThan="5 min"/>
      <update startup="never" trigger="allow"/>
      <tdm rewrite="test" rescan="0 3 2 * * ? *" trigger="allow"/>
      <gribConfig datasetTypes="TwoD Best"/>
    </featureCollection>

    <dataset name="NCEP Climate Forecast System Version 2 (CFSv2) Monthly Products (ds094.2)">

      <featureCollection name="NCEP Climate Forecast System Version 2 (CFSv2) Monthly Products (ds094.2)" featureType="GRIB2" harvest="true" path="grib/ds094.2/timeseries">
        <metadata inherited="true">
          <documentation type="summary">NCEP Climate Forecast System Version 2 (CFSv2) Monthly Products (ds094.2)</documentation>
          <dataFormat>GRIB-2</dataFormat>
        </metadata>

        <collection spec="/glade/p/rda/data/ds094.2/timeseries/.*grb2$"
                    name="ds094.2"
                    timePartition="directory"
                    olderThan="5 min"/>
        <update startup="never" trigger="allow"/>
        <tdm rewrite="test" rescan="0 6 2 * * ? *" trigger="allow"/>
        <gribConfig datasetTypes="TwoD Best"/>
      </featureCollection>

      <featureCollection name="NCEP Climate Forecast System Version 2 (CFSv2) Monthly Products (ds094.2)" featureType="GRIB2" harvest="true" path="grib/ds094.2/diurnal">
        <metadata inherited="true">
          <documentation type="summary">NCEP Climate Forecast System Version 2 (CFSv2) Monthly Products (ds094.2)</documentation>
          <dataFormat>GRIB-2</dataFormat>
        </metadata>

        <collection spec="/glade/p/rda/data/ds094.2/timeseries/.*grb2$"
                    name="ds094.2"
                    timePartition="directory"
                    olderThan="5 min"/>
        <update startup="never" trigger="allow"/>
        <tdm rewrite="test" rescan="0 6 2 * * ? *" trigger="allow"/>
        <gribConfig datasetTypes="TwoD Best"/>
      </featureCollection>
    </dataset>

   </dataset>
</catalog>
