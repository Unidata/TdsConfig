#
# Contains LDM pqacts for the model output served by the TDS
#   catalog testDatasets.xml
#
# =============================================
# = NCEP model data available via IDD - LARGE =
# =============================================
#
# -----------------------------------
# - Global Forecasting System (GFS) -
# -----------------------------------
#
# GFS Global 02.5 degree
#
CONDUIT	^data/nccf/com/gfs/para/gfs\.(........)(..).*pgrb2\.0p25\. 
	FILE
	${DATA_DIR}/native/grid/NCEP/GFS/Global_0p25deg/GFS_Global_0p25deg_\1_\200.grib2
# ----------------------------------------------------
# - Multi-Radar, Multi-Sensor (MRMS) System products -
# ----------------------------------------------------
#
# MRMS Precipitation Products
#
NGRID	^YAU[DP][0-9][0-9] KWNR (..)(....)
	FILE
	${DATA_DIR}/native/grid/NCEP/MRMS/Precip/MRMS_Precip_(\1:yyyy)(\1:mm)\1_\2.grib2
#
# MRMS Model Parameters (on different grid)
#
NGRID	^YAUM[0-9][0-9] KWNR (..)(....)
	FILE
	${DATA_DIR}/native/grid/NCEP/MRMS/Model/MRMS_Model_(\1:yyyy)(\1:mm)\1_\2.grib2
#
# MRMS Lightning Products from NLDN
#
NGRID	^YAUL[0-9][0-9] KWNR (..)(....)
	FILE
	${DATA_DIR}/native/grid/NCEP/MRMS/NLDN/MRMS_NLDN_(\1:yyyy)(\1:mm)\1_\2.grib2
#
# MRMS Rotation Track Products (on different grid)
#
NGRID	^YAUS0[0-9] KWNR (..)(....)
	FILE
	${DATA_DIR}/native/grid/NCEP/MRMS/RotationTrack/MRMS_RotationTrack_(\1:yyyy)(\1:mm)\1_\2.grib2
#
# MRMS Merged Base Reflectivity
#
NGRID	^YAUQ[0-9][0-9] KWNR (..)(....)
	FILE
	${DATA_DIR}/native/grid/NCEP/MRMS/BaseRef/MRMS_BaseReflectivity_(\1:yyyy)(\1:mm)\1_\2.grib2
#
# MRMS Radar Products
#
NGRID	^YAU(C[0-9]|S[1-9])[0-9] KWNR (..)(....)
	FILE
	${DATA_DIR}/native/grid/NCEP/MRMS/Radar/MRMS_Radar_(\2:yyyy)(\2:mm)\2_\3.grib2
#
# MRMS Anything else (mainly future proofing)
#
NGRID	^YAU([ABE-KNORT-Z][0-9][0-9]) KWNR (..)(....)
	FILE
	${DATA_DIR}/native/grid/NCEP/MRMS/Other/MRMS_Other_(\1:yyyy)(\1:mm)\1_\2.grib2
#
# ---------------------------------------------
# - NCEP High Resolution Rapid Refresh (HRRR) -
# ---------------------------------------------
#
# High Resolution Radid Refresh (HRRR) grid output from NCEP
# Based on http://www.nws.noaa.gov/os/notification/tin14-28hrrr-aac.htm
#
# for T1 T2 A1 A2 ii, limiting to A2 = A should result in only
# 00 hour analysis grid, according to:
#
#  http://www.nco.ncep.noaa.gov/pmb/docs/on388/appendixa.html
#
NGRID	^Y.CA.* KWBY (..)(..)
	FILE
	${DATA_DIR}/native/grid/NCEP/HRRR/CONUS_2p5km_Analysis/HRRR_CONUS_2p5km_ana_(\1:yyyy)(\1:mm)\1_\200.grib2
#
# --------------------------------
# - Global Forecast System (GFS) -
# --------------------------------
#
# GFS Global 0.5 degree
#
CONDUIT	^data/nccf/com/gfs/prod/gfs\.(........)(..).*pgrb2\.0p50\.a
	FILE
	${DATA_DIR}/native/grid/NCEP/GFS/Global_0p5deg_Analysis/GFS_Global_0p5deg_ana_\1_\200.grib2
#
# ECMWF grid on global 2.5 x 2.5 degree grid
HRS     ^H.[A-L]... ECMF (..)(..)
	PIPE    decoders/gribtonc
	etc/cdl/ecmwf_1-12.cdl
	${DATA_DIR}/decoded/netcdf/grid/ECMWF/Global_2p5/ECMWF_Global_2p5_(\1:yyyy)(\1:mm)\1_\200.nc
