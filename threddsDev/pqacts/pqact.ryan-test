#
# Contains LDM pqacts for the model output served by the TDS
#   catalog newPointObs.xml
#
# ======================================
# = Observation data available via IDD =
# ======================================
#
# ----------------------------
# - Decoded Observation Data -
# ----------------------------
#
# Metar Station Data 
#
DDS|IDS	^S[AP].... .... ([0-3][0-9])([0-2][0-9])
#	FILE	-metadata	-close
#	${DATA_DIR}/native/test/metar_(\1:yyyy)(\1:mm)\1_\200.dat
	PIPE	-metadata
	etc/TDS/ldm-alchemy/metar2nc.py -o ${DATA_DIR}/decoded/netcdf/surface/metar-test
#
# Surface Buoy Point Data
#
DDS|IDS	^S[IMNS].... .... ([0-3][0-9])([0-2][0-9])
	FILE
	${DATA_DIR}/native/text/buoy/buoy_(\1:yyyy)(\1:mm)\1_\200.txt
#
# Surface Synoptic Point Data
#
DDS|IDS	^SM.... .... ([0-3][0-9])([0-2][0-9])
	FILE
	${DATA_DIR}/native/text/syn/synoptic_(\1:yyyy)(\1:mm)\1_\200.txt
#
# Upper Air Point Data
#
DDS|IDS	^U[EFGHIKLMPQSYZ].... .... ([0-3][0-9])([0-2][0-9])
	FILE
	${DATA_DIR}/native/text/upper_air/upper_air_(\1:yyyy)(\1:mm)\1_\200.txt
DDS|IDS	^U[^EFGHIKLMPQSYZ].... .... ([0-3][0-9])([0-2][0-9])
	FILE
	${DATA_DIR}/native/text/upper_air_other/upper_air_other_(\1:yyyy)(\1:mm)\1_\200.txt
#
# BUFR
#
HRS	(^IS....) (....) ([0-3][0-9])([0-2][0-9])[0-9][0-9]
	FILE
	${DATA_DIR}/native/bufr/obs/surface/\2_\1_(\3:yyyy)(\3:mm)\3_\400.bufr

HRS	(IU....) ([^K]...) ([0-3][0-9])([0-2][0-9])[0-9][0-9]
	FILE
	${DATA_DIR}/native/bufr/obs/upper_air/\2_\1_(\3:yyyy)(\3:mm)\3_\400.bufr

HRS	(IU.[^N]..) (K[^A]..) ([0-3][0-9])([0-2][0-9])[0-9][0-9]
	FILE
	${DATA_DIR}/native/bufr/obs/upper_air/\2_\1_(\3:yyyy)(\3:mm)\3_\400.bufr

HRS	(^I[^SUX]....) (....) ([0-3][0-9])([0-2][0-9])[0-9][0-9]
	FILE
	${DATA_DIR}/native/bufr/obs/other/\2_\1_(\3:yyyy)(\3:mm)\3_\400.bufr

HRS	(^JU....) (....) ([0-3][0-9])([0-2][0-9])[0-9][0-9]
	FILE
	${DATA_DIR}/native/bufr/forecast/upper_air/\2_\1_(\3:yyyy)(\3:mm)\3_\400.bufr

HRS	(^J[^U]....) (....) ([0-3][0-9])([0-2][0-9])[0-9][0-9]
	FILE
	${DATA_DIR}/native/bufr/forecast/other/\2_\1_(\3:yyyy)(\3:mm)\3_\400.bufr
#
# Text Front Products
#
DDS|IDS	^ASUS01 (....) ([0-3][0-9])([0-2][0-9])[0-9][0-9]
	FILE
	${DATA_DIR}/native/text/fronts/Fronts_\1_(\2:yyyy)(\2:mm)\2_\300.txt
DDS|IDS	^ASUS02 (....) ([0-3][0-9])([0-2][0-9])[0-9][0-9]
	FILE
	${DATA_DIR}/native/text/fronts/Fronts_highres_\1_(\2:yyyy)(\2:mm)\2_\300.txt
#
# Tropical Weather Products
