<?xml version="1.0" encoding="UTF-8"?>
<catalog name="THREDDS Server Catalog for Restricted Access Dataset Testing!"
         xmlns="http://www.unidata.ucar.edu/namespaces/thredds/InvCatalog/v1.0"
         xmlns:xlink="http://www.w3.org/1999/xlink"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://www.unidata.ucar.edu/namespaces/thredds/InvCatalog/v1.0
           http://www.unidata.ucar.edu/schemas/thredds/InvCatalog.1.0.6.xsd">

  <datasetRoot path="test" location="content/testdata/" />

  <datasetScan name="Test Digest Authentication Dataset" ID="testDigestAuthDataset"
               path="testDigestAuthDataset" location="content/testdata" restrictAccess="authTesterUser">
    <metadata inherited="true">
      <dataType>Grid</dataType>
    </metadata>

    <filter>
      <include wildcard="*.nc"/>
    </filter>
  </datasetScan>
</catalog>
