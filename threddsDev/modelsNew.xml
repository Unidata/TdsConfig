    <dataset name="NCEP Model Data">

      <dataset name="Global Forecast System (GFS)">

        <metadata inherited="true">
          <documentation type="summary">NCEP Global Forecast System Model, previously called AVN/MRF (Medium Range Forecast)</documentation>
          <documentation xlink:href="http://meted.ucar.edu/nwp/pcu2/avintro.htm"
                         xlink:title="COMET MetEd (Meteorology Education and Training) documentation"/>
          <documentation xlink:href="http://www.nco.ncep.noaa.gov/pmb/products/gfs/"
                         xlink:title="NCEP Model Notes"/>
        </metadata>

        <featureCollection name="GFS-CONUS_40km" featureType="GRIB2" harvest="true" path="grib/NCEP/GFS/CONUS_40km">
          <dataFormat>GRIB-2</dataFormat>
          <!-- metadata inherited="true">
            <documentation type="summary">NCEP GFS Model : AWIPS 212 (H) Grid. Regional - CONUS - Double 
              Resolution (Lambert Conformal).
              Model runs are made at 0, 6, 12, and 18Z, with analysis and forecasts every 6 hours out 5 days.
              Horizontal = 129 by 85 points, resolution 40 km, Lambert Conformal projection.
              Vertical = 1000 to 100 hPa mandatory pressure levels (10 levels); surface, height above ground, pressure
              layers.
            </documentation>
          </metadata -->

          <collection spec="/data/ldm/pub/native/grid/NCEP/GFS/CONUS_40km/GFS_CONUS_40km_#yyyyMMdd_HHmm#.grib2"
                      timePartition="file"
                      olderThan="5 min"/>
          <update startup="never" trigger="allow"/>
          <tdm rewrite="test" rescan="0 8,23,38,53 * * * ? *" trigger="allow"/>
        </featureCollection>


        <featureCollection name="GFS-CONUS_95km_grib2" featureType="GRIB2" harvest="true" path="grib/NCEP/GFS/CONUS_95km_grib2">
          <dataFormat>GRIB-2</dataFormat>
          <!-- metadata inherited="true">
            <documentation type="summary">NCEP GFS Model : AWIPS 213 (H) Grid. National - CONUS - Double Resolution (polar
              stereographic).
              Model runs are made at 0, 6, 12, and 18Z, with analysis and forecasts every 6 hours out 5 days.
              Horizontal = 129 by 85 points, resolution 95.25 km, Polar Stereographic projection.
              Vertical = 1000 to 100 hPa mandatory pressure levels (10 levels); surface, height above ground, pressure
              layers.
            </documentation> 
          </metadata -->

          <collection spec="/data/ldm/pub/native/grid/NCEP/GFS/CONUS_95km_grib2/GFS_CONUS_95km_#yyyyMMdd_HHmm#.grib2"
                      timePartition="file"
                      olderThan="5 min"/>
          <update startup="never" trigger="allow"/>
          <tdm rewrite="test" rescan="0 8,23,38,53 * * * ? *" trigger="allow"/>
        </featureCollection>

      </dataset><!-- end of GFS -->
    </dataset><!-- end of NCEP -->

